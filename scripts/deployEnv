#!/bin/bash
repoPath=$(dirname $(dirname "$(readlink -f "$0")"))
scriptPath=$repoPath/scripts
source $scriptPath/common

#generate config links, backup old
rsync -avnr --backup-dir=${repoPath}/backups/$(date +%Y-%M-%d) $scriptPath/dot/ ~/
installUtilities
installVundleVimPluginManager
#generateSshKeys
